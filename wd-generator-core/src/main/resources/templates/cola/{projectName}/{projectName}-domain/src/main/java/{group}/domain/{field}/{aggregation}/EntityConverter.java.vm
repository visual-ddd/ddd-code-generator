#parse("templates/ClassFunction.vm")
package ${IMPORT_PACKAGE_MAP.get("${ENTITY_CLASS_NAME}Converter")};

import cn.hutool.json.JSONUtil;

#comment("${ENTITY_CLASS_DESCRIPTION}-实体JSON转换器")
public class ${ENTITY_CLASS_NAME}Converter {

    public String convertToDatabaseColumn(${ENTITY_CLASS_NAME} entity) {
        return JSONUtil.toJsonStr(entity);
    }

    public ${ENTITY_CLASS_NAME} convertToEntityAttribute(String value) {
        return JSONUtil.toBean(value, ${ENTITY_CLASS_NAME}.class);
    }
}