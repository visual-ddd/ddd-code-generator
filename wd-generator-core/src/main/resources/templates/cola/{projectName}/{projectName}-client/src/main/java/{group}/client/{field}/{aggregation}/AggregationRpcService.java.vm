#foreach ($query in ${AGGREGATION_QUERY_LIST})

    /**
     * ${query.umlClass.classDesc}
     */
    ResultDTO<${query.QUERY_RESULT_TYPE}> ${CASE_FORMAT_LOWER_CAMEL.convert(${query.umlClass.className})}(${AGGREGATION_CLASS_NAME}Query query);
