#foreach ($pageQuery in ${AGGREGATION_PAGE_QUERY_LIST})

    /**
     * ${pageQuery.umlClass.classDesc}
     */
    ResultDTO<List<${pageQuery.queryResultType}>> ${CASE_FORMAT_LOWER_CAMEL.convert(${pageQuery.umlClass.className})}(${AGGREGATION_CLASS_NAME}PageQuery pageQuery);
